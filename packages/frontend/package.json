  "scripts": {
    "prestart": "npm run export-env:local && npm run build:dependencies",
    "start": "REACT_APP_IS_PRERENDER=false PUBLIC_URL=/evm-debugger react-scripts start",
    "build": "bash build.sh build",
    "build:prod": "bash build.sh build prod",
    "build:stage": " bash build.sh build stage",
    "build:dependencies": "cd ../types; npm run build; cd ../analyzer; npm run build",
    "test": "jest",
    "test:snapshot-update": "jest --updateSnapshot",
    "export-env:local": "export $(cat .env.local | xargs)",
    "create-component": "create-component -p templates",
    "lint:check": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "postbuild": "bash build.sh prerender",
    "postbuild:stage": " bash build.sh prerender stage",
    "postbuild:prod": "bash build.sh prerender prod",
    "serve-prerender": "bash build.sh serve-prerender",
    "serve-prerender:stage": "bash build.sh serve-prerender stage",
    "serve-prerender:prod": "bash build.sh serve-prerender prod",
    "clean": "npm run clean:build && npm run clean:js",
    "clean:build": "rimraf build",
    "clean:js": "rimraf src/**/*.{js,js.map}",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@types/node-fetch": "^2.6.9",
    "@types/react-grid-layout": "^1.3.4",
    "@types/react-gtm-module": "^2.0.1",
    "@types/uuid": "^9.0.7",
    "blueprint-templates-cli": "^0.1.13",
    "dotenv-cli": "^7.2.1",
    "react-test-renderer": "^18.2.0",
    "redux-saga-test-plan": "^4.0.6",
    "webpack-dev-server": "^4.3.1"
  },
  "jest": {
    "verbose": true,
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "globals": {
      "window": {}
    }
  }
}
