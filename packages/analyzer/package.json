{
  "name": "@evm-debuger/analyzer",
  "version": "0.0.2",
  "main": "dist/index.js",
  "scripts": {
    "chain": "hardhat node",
    "build": "tsc -p tsconfig.build.json",
    "start:dev": "npx hardhat run scripts/script.ts --network localhost",
    "test:fetch-abis": "npx tsx scripts/fetchAbisForTests.ts",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "prettier:check": "prettier --config ../../prettierrc.json --ignore-path ../../prettierignore --check .",
    "prettier:fix": "prettier --config ../../prettierrc.json --ignore-path ../../prettierignore --write .",
    "type-check": "tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.1",
    "@typechain/hardhat": "^6.1.3",
    "@types/chai": "^4",
    "@types/jest": "^29.2.4",
    "@types/node-fetch": "^2.6.2",
    "@types/rimraf": "^3",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "ethers": "^5.7.2",
    "hardhat-gas-reporter": "^1.0.9",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@evm-debuger/types": "^0.0.2",
    "evm": "^0.2.0",
    "hardhat": "^2.12.0",
    "node-fetch": "2.6.6",
    "ts-jest": "^29.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
