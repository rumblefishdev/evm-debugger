    "progress": "^2.0.3",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@evm-debuger/analyzer": "^0.1.2",
    "@evm-debuger/config": "^0.0.1",
    "@evm-debuger/types": "^0.1.2",
    "@types/inquirer": "^9.0.3",
    "hardhat": "^2.14.0",
    "inquirer": "^8.2.4",
    "node-fetch": "^3.3.2",
    "solc": "0.8.17"
