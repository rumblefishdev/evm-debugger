{
  "name": "@evm-debuger/transaction-manager",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "start:node": "npx tsx scripts/startNode.ts",
    "start:main": "npx hardhat run scripts/main.ts --network localhost",
    "start:dev": "npx tsx src/index.ts",
    "build": "tsc --build",
