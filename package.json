{
    "name": "evm-debuger",
    "packageManager": "yarn@3.2.4",
    "scripts": {
        "blockchain": "hardhat node",
        "start:dev": "npx hardhat run ./src/index.ts --network localhost",
        "clean": "rimraf build",
        "build": "yarn clean & tsc ./src/*.ts --outDir ./build",
        "start": "npx hardhat run ./build/index.js --network localhost",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "prettier:check": "prettier --config prettierrc.json --ignore-path prettierignore --check .",
        "prettier:fix": "prettier --config prettierrc.json --ignore-path prettierignore --write .",
        "type-check": "tsc --noEmit"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
        "@nomicfoundation/hardhat-toolbox": "^2.0.0",
        "@nomiclabs/hardhat-ethers": "^2.2.0",
        "@nomiclabs/hardhat-etherscan": "^3.1.1",
        "@typechain/hardhat": "^6.1.3",
        "@types/chai": "^4",
        "@types/mocha": "^10",
        "@types/node-fetch": "^2.6.2",
        "@types/rimraf": "^3",
        "chai": "^4.3.6",
        "dotenv": "^16.0.3",
        "ethers": "^5.7.1",
        "hardhat-gas-reporter": "^1.0.9",
        "mocha": "^10.1.0",
        "nodemon": "^2.0.20",
        "rimraf": "^3.0.2",
        "solidity-coverage": "^0.8.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "evm": "^0.2.0",
        "hardhat": "^2.12.0",
        "node-fetch": "2.6.6"
    }
}
